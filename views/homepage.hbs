{{#each posts as |post|}}


<div>
    <div class="card">
        <div class= "card-content">
            <h5>{{post.title}} </h5>
        </div>
        <div class="card-content">
            <p>{{post.content}} </p>
            <p>user id:{{post.userId}} </p>
        </div>
    </div>
</div>



{{else}}
<h6 class="pb-4 has-text-centered is-italic">there are currently no posts</h6>
{{/each}}